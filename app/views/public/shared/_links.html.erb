<%- if controller_name != 'sessions' %>
  <h5 class = "mr-4">アカウントをお持ちの方はこちら<h5>
  <%= link_to "Log in", new_session_path(resource_name), class: "btn btn-log-in" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <h5 class = "mr-4">アカウントをお持ちでない方はこちら<h5>
  <%= link_to "Sign up", new_registration_path(resource_name), class: "btn btn-sign-up" %><br />
<% end %>